{
  "name": "tiny-files",
  "version": "1.0.0",
  "description": "A tiny file explorer for tiny apps",
  "private": "true",
  "repository": "github:MichaelFedora/tiny-files",
  "homepage": "https://michaelfedora.github.io/tiny-files/#",
  "scripts": {
    "build": "del-cli build && webpack",
    "build-prod": "del-cli build && webpack --mode=production",
    "build:docs": "del-cli docs && webpack --env docs=true",
    "build-prod:docs": "del-cli docs && webpack --mode=production --env docs=true",
    "start": "node scripts/host.js",
    "test": "del-cli build && webpack && node scripts/host.js"
  },
  "keywords": [
    "tiny",
    "files"
  ],
  "author": "Michael Fedora",
  "license": "MIT",
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/filesize": "^5.0.0",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.15",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "del-cli": "^4.0.1",
    "eslint": "^8.8.0",
    "eslint-plugin-vue": "^8.4.1",
    "postcss": "^8.4.6",
    "sass": "^1.49.7",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-vue2": "^1.9.3",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "axios": "^0.25.0",
    "buefy": "^0.9.14",
    "bulma": "^0.9.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3"
  },
  "optionalDependencies": {}
}
